<h1> Search Van </h1>
<ul>
        <form method="post" action = "/search">
        <input type = "search" name ="van_ID" placeholder="Enter Van ID">
        <input type = "submit" value = "Search">
        </form>

    <ul>

        {{#each vendors}}
        <li>
            <a href="/vendors/{{this._id}}">{{this.van_ID}}</a>
        </li>
        {{/each}}
        
    </ul>
</ul>